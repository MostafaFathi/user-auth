{
    "name": "mostafafathi/user-auth",
    "description": "Laravel package for user type management with SSO and OTP authentication",
    "type": "library",
    "keywords": ["laravel", "authentication", "sso", "otp", "saml", "openid"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mostafa Fathi",
            "email": "mostafa.fathi.hilles@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "MostafaFathi\\UserAuth\\": "src/",
            "app\\Http\\Controllers\\": "src/Http/Controllers"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MostafaFathi\\UserAuth\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MostafaFathi\\UserAuth\\UserAuthServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
